/* pages/outSouMana/select.wxss */
.main_v{
  width: 100%;
  min-height: 100vh;
  background-color: #F8F8F8;
}
.main_v .title_v{
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 15px;
  font-weight: bold;
  background-color: #F8F8F8;
}
.main_v .back_but_v{
  height: 40px;
  line-height: 40px;
  margin-top: -40px;
  margin-left: 10px;
}
.main_v .but_list_v{
  width: 100%;
  margin-top: 20px;
  padding-top: 1px;
}
.main_v .nos_but_v,
.main_v .pos_but_v{
  width: 80%;
  height: 40px;
  line-height: 40px;
  margin: 30px auto;
  color: #FFF;
  text-align: center;
  background-color: #ff9205;
  border-radius:3px;
}