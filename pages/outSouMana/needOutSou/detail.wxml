<!--pages/outSouMana/needOutSou/detail.wxml-->
<view class="main_v">
  <view class="title_v">需要外包</view>
  <view class="back_but_v" data-pageflag="{{listPageFlag}}" catchtap="goPage">{{backButSign}}</view>
  <view class="thanks_tit_v">您好，感谢您使用工程外包平台</view>
  <view class="thanks_cont_v">如果您想成为外包需求商，请您填写以下内容。我们将会在1-3个工作日内联系您。</view>
  <view class="companyName_v">
    <view class="tit_v">01  公司名称</view>
    <view class="val_v">{{companyName}}</view>
  </view>
  <view class="trade_v">
    <view class="tit_v">02  行业</view>
    <view class="val_v">{{tradeName}}</view>
  </view>
  <view class="otherTrade_v">
    <view class="tit_v">03  其他行业</view>
    <view class="val_v">{{otherTrade}}</view>
  </view>
  <view class="post_v">
    <view class="tit_v">04  岗位名称</view>
    <view class="val_v">{{post}}</view>
  </view>
  <view class="enginName_v">
    <view class="tit_v">05  您所实施的工程名</view>
    <view class="val_v">{{enginName}}</view>
  </view>
  <view class="needCount_v">
    <view class="tit_v">06  您的工程所需人数</view>
    <view class="val_v">{{needCount}}</view>
  </view>
  <view class="speciality_v">
    <view class="tit_v">07  特长</view>
    <view class="val_v">{{speciality}}</view>
  </view>
  <view class="describe_v">
    <view class="tit_v">08  描述</view>
    <view class="ta_v">{{describe}}</view>
  </view>
  <view class="startDate_v">
    <view class="tit_v">09  开始日期</view>
    <view class="picker_v">{{startDate}}</view>
  </view>
  <view class="endDate_v">
    <view class="tit_v">10  结束日期</view>
    <view class="picker_v">{{endDate}}</view>
  </view>
  <view class="contactName_v">
    <view class="tit_v">11  联系人</view>
    <view class="val_v">{{contactName}}</view>
  </view>
  <view class="phone_v">
    <view class="tit_v">12  联系方式</view>
    <view class="val_v">{{phone}}</view>
  </view>
</view>
