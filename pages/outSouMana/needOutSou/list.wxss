.main_v{
  width: 100%;
  background-color: #F8F8F8;
}
.main_v .title_v{
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 15px;
  font-weight: bold;
  background-color: #F8F8F8;
}
.main_v .back_but_v{
  height: 40px;
  line-height: 40px;
  margin-top: -40px;
  margin-left: 10px;
}
.main_v .no_data_v{
  width: 100%;
  height:40px;
  line-height:40px;
  text-align: center;
}
.main_v .list_v{
  width: 95%;
  margin: auto;
}
.main_v .list_v .item_v{
  width: 100%;
  margin:10px auto 0;
  background-color: #FFF;
  padding: 10px 0 10px;
}
.main_v .list_v .item_v .row_v{
  width: 90%;
  margin-left:20px;
}
.main_v .list_v .item_v .row_v .key_v,
.main_v .list_v .item_v .row_v .val_v{
  width: 90%;
  height: 30px;
  line-height: 30px;
}